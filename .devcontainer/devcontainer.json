{
    "name": "SGE",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "apache",
    "workspaceFolder": "/var/www/html",
    "workspaceMount": "source=${localWorkspaceFolder},target=/var/www/html,type=bind",
    "postCreateCommand": "if [ ! -f vendor/autoload.php ]; then composer install --no-interaction; fi",
    "forwardPorts": [80, 8080, 3306],
    "portsAttributes": {
        "80": {
            "label": "Application",
            "onAutoForward": "notify"
        },
        "8080": {
            "label": "phpMyAdmin",
            "onAutoForward": "silent"
        },
        "3306": {
            "label": "MySQL",
            "onAutoForward": "silent"
        }
    }
}
